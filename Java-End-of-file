import java.util.*;
import java.io.*;
import java.util.Scanner;

public class Solution {

    public static void main(String[] args) {
       Scanner sc = new Scanner(System.in);
                 
                 String r ="";
                 String v = "";
                 int j = 1;
                 
                 while(sc.hasNext()){//retorna true, se ainda existir alguma linha embaixo e false se a linha estiver vazia
          
                     v = sc.nextLine();
                     r +=j+" "+v+"\n";
                     
                     j+=1;
                     
                 }
                 System.out.println(r);
          
    }
}
